    },
    "location": {
      "type": "string"
    },
    "visibility_date": {
      "type": "datetime"
    },
    "registration_limit_date": {
      "type": "datetime"
    },
    "duration": {
      "type": "time"
    },
    "password": {
      "type": "string"
    },
    "event_date": {
      "type": "datetime"
    },
    "registrations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::registration.registration"
    },
    "ressources": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ressource.ressource"
    },
    "description": {
      "type": "text"
    },
    "event_img": {
      "allowedTypes": [
